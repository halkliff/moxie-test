{
  "name": "@moxie/take-home-assignment",
  "version": "1.0.0",
  "description": "Take-home assignment for Moxie recruitment",
  "author": "Werberth Lins <werberth.lins@gmail.com>",
  "main": "main.js",
  "type": "module",
  "packageManager": "pnpm@9.0.4+sha256.caa915eaae9d9aefccf50ee8aeda25a2f8684d8f9d5c6e367eaf176d97c1f89e",
  "engines": {
    "node": "20.12.2"
  },
  "engineStrict": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed",
    "start": "nest start",
    "start:dev": "nest start --builder swc --watch --type-check",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "jest --config vitest run --config ./vitest.config.e2e.ts"
  },
  "prisma": {
    "seed": "pnpm exec tsx prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "10.3.8",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.3.8",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-fastify": "10.3.8",
    "@prisma/client": "5.12.1",
    "fastify": "4.26.2",
    "qs": "6.12.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.8",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.4.16",
    "@types/node": "20.12.7",
    "@types/qs": "6.9.15",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "@vitest/coverage-v8": "1.5.0",
    "eslint": "8.57.0",
    "eslint-config-love": "47.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "prettier": "3.2.5",
    "prisma": "5.12.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.4",
    "ts-loader": "9.5.1",
    "tsconfig-paths": "4.2.0",
    "tsx": "^4.7.2",
    "typescript": "5.4.5",
    "unplugin-swc": "1.4.5",
    "vitest": "1.5.0"
  }
}
